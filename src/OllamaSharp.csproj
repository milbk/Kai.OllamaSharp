<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <PackageId>Kai.OllamaSharp</PackageId>
    <TargetFramework>net8.0</TargetFramework>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>API bindings for Ollama</Title>
		<Copyright>Andreas Wäscher, Mpkgs</Copyright>
		<PackageIcon>Ollama.png</PackageIcon>
		<Description>
			This project adds bindings to the Ollama API to include offline LLMs into your .NET apps
		</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Version>1.1.0.1</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\Ollama.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>
